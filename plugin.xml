<?xml version='1.0' encoding='UTF-8'?>
<plugin id="3D-touch-home-icon-actions" version="0.0.1" xmlns="http://apache.org/cordova/ns/plugins/1.0">
    <name>App 3D touch home icon actions</name>
    <author>Alaa Al Shamey</author>
    <description></description>
    <keywords></keywords>
    <license></license>
    <platform name="ios">
      <config-file target="*-Info.plist" parent="UIApplicationShortcutItems">
        <array>
            <dict>
                <key>UIApplicationShortcutItemIconFile</key>
                <string>ion-home-outline</string>
                <key>UIApplicationShortcutItemTitle</key>
                <string>سوق مستعمل</string>
                <key>UIApplicationShortcutItemType</key>
                <string>market</string>
            </dict>
            <dict>
                <key>UIApplicationShortcutItemIconFile</key>
                <string>ion-ios-briefcase-outline</string>
                <key>UIApplicationShortcutItemTitle</key>
                <string>إعلاناتك</string>
                <key>UIApplicationShortcutItemType</key>
                <string>myAds</string>
            </dict>
            <dict>
                <key>UIApplicationShortcutItemIconFile</key>
                <string>ion-person-stalker-outline</string>
                <key>UIApplicationShortcutItemTitle</key>
                <string>منتدى مستعمل</string>
                <key>UIApplicationShortcutItemType</key>
                <string>forum</string>
            </dict>
            <dict>
                <key>UIApplicationShortcutItemIconFile</key>
                <string>ion-ios-briefcase-outline</string>
                <key>UIApplicationShortcutItemTitle</key>
                <string>مواضيعك</string>
                <key>UIApplicationShortcutItemType</key>
                <string>myFrs</string>
            </dict>
        </array>
      </config-file>
        <resource-file src="ion-ios-briefcase-outline.png" />
        <resource-file src="ion-home-outline.png" />
        <resource-file src="ion-person-stalker-outline.png" />
    </platform>
</plugin>
